<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>platform-application</key>
    <true/>
    <key>com.apple.private.security.no-container</key>
    <true/>
    <key>com.apple.private.security.storage.AppDataContainers</key>
    <true/>
    <key>com.apple.security.app-sandbox</key>
    <false/>
    
    <!-- HID / Touch Simulation Access -->
    <key>com.apple.private.hid.manager.client</key>
    <true/>
    <key>com.apple.private.hid.client.event-dispatch</key>
    <true/>
    <key>com.apple.private.hid.client.event-filter</key>
    <true/>
    <key>com.apple.private.input.client.event-dispatch</key>
    <true/>
    <key>com.apple.private.input.injection</key>
    <true/>
    
    <!-- SpringBoard / BackBoard Access -->
    <key>com.apple.backboard.client</key>
    <true/>
    <key>com.apple.springboard.launchapplications</key>
    <true/>
    
    <!-- Process Control -->
    <key>run-unsigned-code</key>
    <true/>
    <key>get-task-allow</key>
    <true/>
    <key>task_for_pid-allow</key>
    <true/>
    
    <!-- Unlimited Background (Private) -->
    <key>com.apple.private.multitasking.unlimited-background-time</key>
    <true/>
</dict>
</plist>
